html {
  background: #F7F7F7 !important;
  align-items: center;
  flex: 1;
  height: 100%;
}
html [v-cloak] {
  display: none;
}
.page {
  padding-bottom: 1.6rem;
  height: 100%;
}
.van-nav-bar__title {
  color: #fff !important;
}
.van-nav-bar .van-icon {
  color: #fff !important;
}
.line {
  background: #F7F7F7 !important;
  width: 100%;
  height: 0.266667rem;
}
.confirm-btn {
  margin: 0 auto;
  display: flex !important;
  justify-content: center;
  margin-top: 50px;
}
.order_upload {
  background: #fff;
  padding: 0 0.266667rem;
}
.order_upload .upload_img {
  display: flex;
  flex-wrap: wrap;
}
.order_upload .upload_top {
  padding: 0.266667rem 0;
  font-size: 0.373333rem;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.order_upload .upload_top span {
  font-size: 0.32rem;
  font-family: PingFang SC;
  font-weight: bold;
  color: #999999;
}
